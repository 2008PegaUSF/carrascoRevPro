package questions;

public class Q2 {
	static int n1 = 0, n2 = 1, n3 = 0;
	static void printFibonacci(int count) {
		System.out.print(" " + 0);
		System.out.print(" " + 1);
		if (count > 0) {
			n3 = n1 + n2;
			n1 = n2;
			n2 = n3;
			System.out.print(" " +n3);
			printFibonacci( count - 1);
		}
	}
}
